<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping >
    <class name="com.express.model.User" table="user">
        <id name="id"  type="string">
            <generator class="assigned"/>
        </id>
        <property name="state" type="java.lang.String"/>
        <property name="role" type="java.lang.String"/>
        <property name="loginPassword" type="java.lang.String"/>
        <property name="payPassword" type="java.lang.String"/>
        <property name="mobile" type="string"/>
        <property name="name" type="string"/>
        <property name="sex" type="string"/>
        <property name="identity" type="string"/>
        <property name="path" type="string"/>
        <property name="balance" type="double"/>
        <property name="jifen" type="int"/>
        <property name="pingjia" type="string"/>
        <property name="pingfen" type="float"/>
        <property name="order_count" type="int"/>
        <property name="credit" type="int"/>
        <property name="accept_able" type="boolean"/>
        <property name="send_able" type="boolean"/>
        <property name="longitude" type="double"/>
        <property name="latitude" type="double"/>
        <property name="address" type="string"/>
        <property name="pass" type="boolean"/>
        <property name="forbidTime" type="java.sql.Timestamp"/>
        <property name="unionid" type="string"/>
        <property name="openId" type="string"/>
        <property name="loginStatus" type="string"/>
    </class>
</hibernate-mapping>